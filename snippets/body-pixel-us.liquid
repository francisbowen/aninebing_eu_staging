{%- if template.name == 'product' -%}
<!-- Pinterest Pixel for PageVisit on Product Details for US -->
<script>
  pintrk('track', 'pagevisit', {
    line_items: [{
      product_id: '{{ product.id }}' 
    }]
  });
</script>
<noscript><img height="1" width="1" style="display:none;" alt="" src="https://ct.pinterest.com/v3/?tid=2613667945459&event=pagevisit&noscript=1" /></noscript>
<!-- End Pinterest Pixel for PageVisit on Product Details for US -->
{%- endif -%}

<!-- Pinterest Pixel for AddToCart on Product Details for US -->
<script>
  $(function() {
    $(document).on('Cart:add', function(event, item) {
      pintrk('track', 'AddToCart', {
        line_items: [
          {
            product_id: item.product_id
          }
        ]
      });
    });
  });
</script>
<!-- End Pinterest Pixel for AddToCart on Product Details for US -->

